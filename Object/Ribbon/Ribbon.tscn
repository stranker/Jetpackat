script/source = "extends Node2D

var activated : bool = false
enum RIBBON_TYPE {BLUE, YELLOW, RED}
export (RIBBON_TYPE) var type
export (Array, Color) var colors
export var add_time : int = 0

signal activated(add_time)

func _ready():
	randomize()
	get_random_color()
	connect('activated',get_tree().root.get_node('GameScene'),'add_game_time')
	pass

func get_random_color():
	type = RIBBON_TYPE.values()[rand_range(0,RIBBON_TYPE.values().size())]
	change_color()
	set_add_time()
	pass

func change_color():
	$Sprite.self_modulate = colors[type]
	$Ballon.modulate = colors[type]
	$Ballon2.modulate = colors[type]
	pass

func set_add_time():
	add_time = type + 1
	match add_time:
		1:
			$Sprite/Label.text = 'I'
		2:
			$Sprite/Label.text = 'II'
		3:
			$Sprite/Label.text = 'III'
	pass

func desactivate():
	activated = !activated
	$Anim.play('Deactivate')
	emit_signal('activated',add_time)
	var new_color = Color($Sprite.self_modulate.r,$Sprite.self_modulate.g,$Sprite.self_modulate.b,0.2)
	$Tween.interpolate_property($Sprite,'self_modulate',$Sprite.self_modulate,new_color,0.5,Tween.TRANS_QUAD,Tween.EASE_OUT)
	$Tween.interpolate_property($Ballon,'modulate',$Ballon.modulate, Color($Ballon.modulate.r,$Ballon.modulate.g,$Ballon.modulate.b,0),1,Tween.TRANS_LINEAR,Tween.EASE_OUT,0.5)
	$Tween.interpolate_property($Ballon2,'modulate',$Ballon2.modulate, Color($Ballon2.modulate.r,$Ballon2.modulate.g,$Ballon2.modulate.b,0),1,Tween.TRANS_LINEAR,Tween.EASE_OUT,0.5)
	$Tween.start()
	pass

func _on_EnterArea_body_entered(body):
	if body.is_in_group('Player') and !activated:
		desactivate()
		body.impulse(1200)
	pass # Replace with function body.


func _on_VisibilityNotifier2D_screen_exited():
	queue_free()
	pass # Replace with function body.
"

