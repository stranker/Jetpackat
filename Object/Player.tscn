[sub_resource type="GDScript" id=1]
script/source = "extends KinematicBody2D

var jump_speed : float = 700
var horizontal_speed : float = 270
var velocity : Vector2
var jump_direction : int
var GRAVITY = 1600
var activated : bool = false 
var sizeViewport : Vector2
var sizeSprite : Vector2
var invencible : bool = false
var dead : bool = false

signal start_playing
signal dead
signal current_height(value)

func _ready():
	jump_direction = 1
	$Visible.scale.x = -jump_direction
	sizeViewport = get_viewport_rect().size
	position = Vector2(sizeViewport.x/2,0)
	sizeSprite = $Visible/Skin.texture.get_size() * $Visible/Skin.scale / $Visible/Skin.hframes
	set_physics_process(activated)
	GameManager.player = self
	connect('start_playing',get_tree().root.get_node('GameScene'),'start_game')
	connect('dead',get_tree().root.get_node('GameScene'),'end_game')
	connect('current_height',get_tree().root.get_node('GameScene'),'update_height')
	emit_signal('current_height',abs(position.y))
	pass # Replace with function body.

func _physics_process(delta):
	if activated:
		velocity.y += GRAVITY * delta
		if is_on_floor():
			velocity.x = 0
		move_and_slide(velocity,Vector2(0,-1))
		check_boundaries()
	pass

func check_boundaries():
	if position.x < sizeSprite.x * 0.5:
		position.x = sizeSprite.x * 0.5
	elif position.x > sizeViewport.x - sizeSprite.x * 0.5:
		position.x = sizeViewport.x - sizeSprite.x * 0.5
	pass

func jump():
	velocity.y = -jump_speed
	check_jump_direction()
	$Visible/Jetpack/Anim.play('Propulsion')
	emit_signal('current_height',int(abs(position.y) * 0.01))
	pass

func check_is_active():
	if !activated:
		activated = !activated
		emit_signal('start_playing')
		set_physics_process(activated)
	pass

func check_jump_direction():
	$Visible.scale.x = -jump_direction
	if jump_direction == 1:
		$Visible/Jetpack/CPUParticles2D.emitting = true
	else:
		$Visible/Jetpack/CPUParticles2D2.emitting = true
	velocity.x = jump_direction * horizontal_speed
	pass

func take_damage():
	if !invencible and !dead:
		dead = !dead
		set_physics_process(false)
		$Anim.play('Dead')

func _on_VisibilityNotifier2D_screen_exited():
	if !get_tree().paused:
		take_damage()
	pass # Replace with function body.


func _on_Area_gui_input(event):
	if event is InputEventScreenTouch:
		if !event.is_echo() and event.pressed:
			check_is_active()
			jump_direction = -jump_direction
			jump()
	pass # Replace with function body.


func _on_Invencible_button_down():
	invencible = !invencible
	pass # Replace with function body.
"

