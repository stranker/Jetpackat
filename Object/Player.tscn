script/source = "extends Camera2D


func _ready():
	GameManager.camera = self
	pass # Replace with function body.

func go_to_player_position():
	$Tween.stop_all()
	var sprite_pos = get_parent().global_position
	$Tween.interpolate_property(self,'position',position,(sprite_pos - global_position).normalized() * 50,2,Tween.TRANS_LINEAR,Tween.EASE_OUT)
	$Tween.start()
	pass"

