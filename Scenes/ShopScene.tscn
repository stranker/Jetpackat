script/source = "extends Control

export (PackedScene) var item_shop_panel
var current_item_panel
onready var player = get_node('VBC/PlayerPanel/HBC/Player/Skin')
onready var item_list = get_node('VBC/HBC/ShopPanel/PanelItems/SC/Items')

func _ready():
	update_currency()
	update_equipment()
	_on_Hat_button_down()
	pass

func _input(event):
	if event is InputEventKey and !event.is_echo() and event.is_pressed():
		if event.scancode == KEY_1:
			add_currency()

func add_currency():
	GameManager.coins = 99999
	GameManager.fishes = 99999
	update_currency()
	update_items_state()

func update_items_state():
	for i in item_list.get_children():
		i.check_item_state()
	pass

func update_currency():
	$VBC/HBC/ShopPanel/Currency/CoinPanel/Coins/Label.text = str(GameManager.coins)
	$VBC/HBC/ShopPanel/Currency/FishPanel/Fishes/Label.text = str(GameManager.fishes)
	pass

func _on_Hat_button_down():
	if GameManager.item_data.has('Hat'):
		create_panels(GameManager.item_data['Hat'])
	pass

func _on_Skin_button_down():
	if GameManager.item_data.has('Pattern'):
		create_panels(GameManager.item_data['Pattern'])
	pass

func _on_Scarf_button_down():
	if GameManager.item_data.has('Scarf'):
		create_panels(GameManager.item_data['Scarf'])
	pass # Replace with function body.

func _on_Jetpack_button_down():
	if GameManager.item_data.has('Jetpack'):
		create_panels(GameManager.item_data['Jetpack'])
	pass # Replace with function body.

func clear_item_panels():
	for i in item_list.get_children():
		i.queue_free()
	pass

func create_panels(data : Dictionary):
	clear_item_panels()
	for i in data.keys():
		var isp = item_shop_panel.instance()
		item_list.add_child(isp)
		isp.set_item_info(i,data[i])
	pass

func _on_TextureButton_button_down():
	$AnimationPlayer.play('FadeIn')
	pass

func equip_item(type,item_name,color):
	update_equipment()
	enable_used_items(item_name)
	pass

func enable_used_items(item_name):
	for i in item_list.get_children():
		if i.item_name != item_name:
			i.enable_use_button(true)
	pass

func update_equipment():
	if !GameManager.items_equipped.keys().empty():
		for item_type in GameManager.items_equipped.keys():
			set_item_on_player(item_type)
			update_item_color(item_type,GameManager.items_equipped[item_type])
	pass

func set_item_on_player(item_type):
	var item = GameManager.items_equipped[item_type]
	if item_type != 'Skin':
		if typeof(item) != TYPE_ARRAY:
			player.get_node(item_type).texture = load('Sprites/Shop/Cat/'+item_type+'/'+item+'.png')
		else:
			player.get_node(item_type).texture = load('Sprites/Shop/Cat/'+item_type+'/'+item[0]+'.png')
		player.get_node(item_type).show()
	pass

func open_color_picker(item_panel):
	$VBC/HBC/ShopPanel/PanelItems/CustomColor.show()
	current_item_panel = item_panel
	pass

func update_buttons_states(type,item):
	for i in item_list.get_children():
		i.check_item_state()


func _on_CustomColor_value_changed(c):
	if current_item_panel and typeof(current_item_panel) != TYPE_STRING:
		current_item_panel.set_item_color(c)
		update_item_color(current_item_panel.type,c)
	else:
		if player:
			update_item_color('Skin',c)
	pass # Replace with function body.

func update_item_color(item_type, _color):
	if typeof(GameManager.items_equipped[item_type]) == TYPE_ARRAY:
		var color = _color
		if typeof(_color) == TYPE_ARRAY:
			color = _color[1]
		if item_type != 'Skin':
			player.get_node(item_type).self_modulate = color
		else:
			player.self_modulate = color
			$VBC/PlayerPanel/HBC/Player/SkinColorButton.self_modulate = color
			GameManager.items_equipped['Skin'][1] = color
	else:
		player.get_node(item_type).self_modulate = 'ffffff'
	pass

func _on_SkinColorButton_button_down():
	$VBC/HBC/ShopPanel/PanelItems/CustomColor.show()
	current_item_panel = 'Skin'
	pass # Replace with function body.

func unequip_item(item_type):
	player.get_node(item_type).texture = null
	pass

func equip_default_jetpack():
	for i in item_list.get_children():
		if i.type == 'Jetpack' and i.item_name == 'Green':
			i.equip_item()
			break
	pass

func item_preview(item_type,item_texture):
	for i in item_list.get_children():
		i.unequip_item()
	player.get_node(item_type).texture = item_texture
	player.get_node(item_type).self_modulate = 'ffffff'
	pass

func _on_AnimationPlayer_animation_finished(anim_name):
	if anim_name == 'FadeIn':
		get_tree().change_scene('Scenes/MainMenu.tscn')
	pass # Replace with function body.
"

