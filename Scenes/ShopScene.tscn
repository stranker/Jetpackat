[sub_resource type="GDScript" id=10]
script/source = "extends Control

export (NodePath) var item_confirmation_path
export (NodePath) var currency_multiplier_price_tag
export (NodePath) var magnetchip_price_tag
export (Texture) var coin_texture
export (Texture) var fish_texture
export (Color) var level_max_color
var item_confirmation
var button_clicked_ref = null
signal upgrade_closed

func _ready():
	item_confirmation = get_node(item_confirmation_path)
	update_price_tags()
	hide()
	pass

func enter():
	$UpgradePanelAnim.play('Enter')
	pass

func _on_CurrencyButton_on_clicked(button_ref):
	$BackPanel/Background/ChipButton.unclick_button()
	var currency_data = ItemManager.get_upgrade_item_by_type(ItemManager.UpgradeType.CURRENCY)
	set_item_confirmation(currency_data)
	button_clicked_ref = button_ref
	button_clicked_ref.set_item(currency_data)
	pass

func _on_ChipButton_on_clicked(button_ref):
	$BackPanel/Background/CurrencyButton.unclick_button()
	var magnetchip_data = ItemManager.get_upgrade_item_by_type(ItemManager.UpgradeType.CHIP)
	set_item_confirmation(magnetchip_data)
	button_clicked_ref = button_ref
	button_clicked_ref.set_item(magnetchip_data)
	pass

func _on_UpgradeCloseButton_button_down():
	$UpgradePanelAnim.play_backwards('Enter')
	emit_signal('upgrade_closed')
	pass

func set_item_confirmation(item):
	var item_price_data = item.get_next_level_info()
	if item_price_data:
		item_confirmation.set_shop_info(item.item_name, item_price_data['payment'], item_price_data['price'], self)
	pass

func change_to_buy_state():
	button_clicked_ref.upgrade_item()
	update_ui()
	pass

func update_ui():
	get_parent().get_parent().update_currency()
	var currency = ItemManager.get_upgrade_item_by_type(ItemManager.UpgradeType.CURRENCY)
	var magnet = ItemManager.get_upgrade_item_by_type(ItemManager.UpgradeType.CHIP)
	$BackPanel/Background/CurrencyDescription/Level.text = 'LVL. ' + (str(currency.item_level) if currency.item_level != currency.item_price_per_level.size() else \"MAX\")
	$BackPanel/Background/ChipDescription/Level.text = 'LVL. ' + (str(magnet.item_level) if magnet.item_level != magnet.item_price_per_level.size() else \"MAX\") 
	$BackPanel/Background/CurrencyButton.unclick_button()
	$BackPanel/Background/ChipButton.unclick_button()
	update_price_tags()
	pass

func _on_ShopItemConfirmation_exit_viewport():
	update_ui()
	pass # Replace with function body.

func update_price_tags():
	var currency_multiplier_info : Dictionary = ItemManager.get_upgrade_item_by_type(ItemManager.UpgradeType.CURRENCY).get_next_level_info()
	var magnet_info : Dictionary = ItemManager.get_upgrade_item_by_type(ItemManager.UpgradeType.CHIP).get_next_level_info()
	if currency_multiplier_info:
		get_node(currency_multiplier_price_tag).get_node('Price/Payment').texture = get_payment_texture(currency_multiplier_info['payment'])
		get_node(currency_multiplier_price_tag).get_node('Price').text = str(currency_multiplier_info['price'])
	else:
		get_node(currency_multiplier_price_tag).get_node('Price/SoldOut').show()
		get_node(currency_multiplier_price_tag).get_node('Price/Payment').texture = null
		$BackPanel/Background/CurrencyDescription/Level.self_modulate = level_max_color
	if magnet_info:
		get_node(magnetchip_price_tag).get_node('Price/Payment').texture = get_payment_texture(magnet_info['payment'])
		get_node(magnetchip_price_tag).get_node('Price').text = str(magnet_info['price'])
	else:
		get_node(magnetchip_price_tag).get_node('Price/Payment').texture = null
		get_node(magnetchip_price_tag).get_node('Price/SoldOut').show()
		$BackPanel/Background/ChipDescription/Level.self_modulate = level_max_color
	pass

func get_payment_texture(type : int):
	match type:
		ItemManager.Payment.COIN:
			return coin_texture
		ItemManager.Payment.FISH:
			return fish_texture
	pass"

