script/source = "extends Control

export (PackedScene) var item_shop_panel
var current_item_panel
onready var player = get_node('MainPanel/ShopPanel/BackPanel/HBC/PlayerPanel/Player')
onready var item_list = get_node('MainPanel/ShopPanel/BackPanel/HBC/PanelItems/ItemPageGrid/VBC/Grid')
onready var coin_label = get_node('VBC/CurrencyInfo/HBC/Coins/Panel/Cuant')
onready var fish_label = get_node('VBC/CurrencyInfo/HBC/Fish/Panel/Cuant')
onready var color_picker = get_node('MainPanel/ShopPanel/BackPanel/HBC/PanelItems/CustomColor')

func _ready():
	update_currency()
	update_equipment()
	_on_HatButton_button_down()
	$TransitionScene.play_random_backwards()
	pass

func _input(event):
	if event is InputEventKey and !event.is_echo() and event.is_pressed():
		if event.scancode == KEY_1:
			add_currency()

func add_currency():
	GameManager.coins = 99999
	GameManager.fishes = 99999
	update_currency()
	update_items_state()
	pass

func update_items_state():
	for i in item_list.get_children():
		i.check_item_state()
	pass

func update_currency():
	coin_label.text = str(GameManager.coins)
	fish_label.text = str(GameManager.fishes)
	pass

func create_panels(data : Array):
	$MainPanel/ShopPanel/BackPanel/HBC/PanelItems/ItemPageGrid.create_shop_items(data)
	pass

func _on_TextureButton_button_down():
	ItemManager.save_data()
	$TransitionScene.play_random()
	pass

func equip_item(item):
	update_equipment()
	enable_used_items(item)
	pass

func enable_used_items(item):
	for i in item_list.get_children():
		if i.item != item:
			i.enable_use_button(true)
	pass

func update_equipment():
	if !ItemManager.items_equipped.empty():
		for item_equipped in ItemManager.items_equipped:
			set_item_on_player(item_equipped)
	pass

func set_item_on_player(item_equipped):
	var item = ItemManager.get_item_by_type_id(int(item_equipped.item_type),int(item_equipped.item_id))
	if item.item_type != ItemManager.ItemType.SKIN:
		var player_item = player.get_node(ItemManager.itemTypeToString[item.item_type])
		player_item.texture = load(item.item_image_location)
		if item.has_color():
			player_item.self_modulate = item.item_color
		player_item.show()
	else:
		var skin = player.get_node('Skin')
		skin.texture = load(item.item_image_location)
		skin.self_modulate = item.item_color
	pass

func open_color_picker(item_panel):
	color_picker.show()
	current_item_panel = item_panel
	pass

func update_buttons_states(item):
	for i in item_list.get_children():
		i.check_item_state()
	update_currency()
	pass

func _on_CustomColor_value_changed(c):
	if current_item_panel:
		if typeof(current_item_panel) != TYPE_STRING:
			current_item_panel.set_item_color(c)
		else:
			set_skin_color(c)
	pass # Replace with function body.

func change_color_item(item):
	player.get_node(ItemManager.itemTypeToString.get(item.item_type)).self_modulate = item.item_color
	pass

func set_skin_color(color):
	player.get_node('Skin').self_modulate = color
	ItemManager.set_skin_color(color)
	pass

func unequip_item(item):
	player.get_node(ItemManager.itemTypeToString.get(item.item_type)).texture = null
	pass

func equip_default_jetpack():
	for i in item_list.get_children():
		if i.item.item_type == ItemManager.ItemType.JETPACK and i.item.item_name == 'Green':
			i.equip_item()
			break
	pass

func item_preview(item_type,item_texture):
	for i in item_list.get_children():
		i.unequip_item()
	player.get_node(ItemManager.itemTypeToString.get(item_type)).texture = item_texture
	player.get_node(ItemManager.itemTypeToString.get(item_type)).self_modulate = 'ffffff'
	pass

func _on_Anim_animation_finished(anim_name):
	if !$TransitionScene.has_played:
		get_tree().change_scene('Scenes/MainMenu.tscn')
	pass # Replace with function body.

func _on_SkinButton_button_down():
	on_item_type_selected(ItemManager.ItemType.SKIN)
	unclick_other_buttons($MainPanel/ShopPanel/BackPanel/HBC/PlayerPanel/HatButton)
	pass # Replace with function body.

func _on_HatButton_button_down():
	on_item_type_selected(ItemManager.ItemType.HAT)
	unclick_other_buttons($MainPanel/ShopPanel/BackPanel/HBC/PlayerPanel/HatButton)
	pass # Replace with function body.

func _on_PatternButton_button_down():
	on_item_type_selected(ItemManager.ItemType.PATTERN)
	unclick_other_buttons($MainPanel/ShopPanel/BackPanel/HBC/PlayerPanel/PatternButton)
	pass # Replace with function body.

func _on_JetpackButton_button_down():
	on_item_type_selected(ItemManager.ItemType.JETPACK)
	unclick_other_buttons($MainPanel/ShopPanel/BackPanel/HBC/PlayerPanel/JetpackButton)
	pass # Replace with function body.

func _on_ScarfButton_button_down():
	on_item_type_selected(ItemManager.ItemType.SCARF)
	unclick_other_buttons($MainPanel/ShopPanel/BackPanel/HBC/PlayerPanel/ScarfButton)
	pass # Replace with function body.

func on_item_type_selected( type ):
	if type != ItemManager.ItemType.SKIN:
		var items : Array = ItemManager.get_items_by_type(type)
		if !items.empty():
			create_panels(items)
	else:
		current_item_panel = \"SKIN\"
		open_color_picker(current_item_panel)
	pass

func unclick_other_buttons(current_button):
	for button in $MainPanel/ShopPanel/BackPanel/HBC/PlayerPanel.get_children():
		if button is TextureButton:
			if button != current_button:
				button.unclick_button()
	pass

func _on_CloseButton_button_down():
	$MainPanel/ShopPanel/ShopPanelAnim.play_backwards('Enter')
	if $ShopItemConfirmation.visible:
		$ShopItemConfirmation.exit_viewport()
	$VBC/CurrencyInfo/HBC/ClosePanel/CloseButton/Anim.play('FadeInCloseButton')
	pass # Replace with function body.

func _on_ShopButton_button_clicked():
	$MainPanel/ShopPanel/ShopPanelAnim.play('Enter')
	$MainPanel/ShopPanel/BackPanel/HBC/PlayerPanel/HatButton._on_ButtonShop_button_down()
	$VBC/CurrencyInfo/HBC/ClosePanel/CloseButton/Anim.play_backwards('FadeInCloseButton')
	pass # Replace with function body.

func _on_WorkshopButton_button_clicked():
	$MainPanel/UpgradePanel.enter()
	$VBC/CurrencyInfo/HBC/ClosePanel/CloseButton/Anim.play_backwards('FadeInCloseButton')
	pass # Replace with function body.

func show_shop_confirmation(item, panel_ref):
	$ShopItemConfirmation.set_shop_info(item.item_name, item.item_payment, item.item_price, panel_ref)
	pass

func show_workshop_confirmation(item, cost):
	$ShopItemConfirmation.set_workshop_info(item.item_name, cost)
	pass

func _on_UpgradePanel_upgrade_closed():
	$VBC/CurrencyInfo/HBC/ClosePanel/CloseButton/Anim.play('FadeInCloseButton')
	pass # Replace with function body.
"

