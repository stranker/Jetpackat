script/source = "extends Panel

signal config_panel_closed

func _on_ConfigPanel_visibility_changed():
	if visible:
		set_sliders()
		$BackPanel/VBC/Language/LanguageButton.texture_normal = load('res://Sprites/UI/MainMenu/'+GameManager.language+'_flag.png')
	pass # Replace with function body.

func set_sliders():
	$BackPanel/VBC/Music/MusicSlider.value = GameManager.music_volume
	$BackPanel/VBC/Sound/SoundSlider.value = GameManager.sound_volume

func _on_MusicSlider_value_changed(value):
	GameManager.change_music_volume(int(value))
	pass # Replace with function body.

func _on_SoundSlider_value_changed(value):
	GameManager.change_sound_volume(int(value))
	pass # Replace with function body.

func _on_Mode_toggled(button_pressed):
	GameManager.left_mode = button_pressed
	pass # Replace with function body.

func _on_LanguageButton_button_down():
	$Anim.play('Language')
	$CloseConfigButton.disabled = true
	$LanguageSelection/Tween.interpolate_property($BackPanel/VBC/Language/LanguageButton,
		'rect_scale',Vector2(0.5,0.5),Vector2(1,1),0.5,Tween.TRANS_BOUNCE,Tween.EASE_OUT)
	$LanguageSelection/Tween.start()
	pass # Replace with function body.

func _on_LanguageSelection_language_selected(texture):
	$BackPanel/VBC/Language/LanguageButton.texture_normal = texture
	$Anim.play_backwards('Language')
	$CloseConfigButton.disabled = false
	pass # Replace with function body.

func _on_CloseConfigButton_button_down():
	GameManager.save_current_game_info()
	emit_signal('config_panel_closed')
	pass # Replace with function body.
"

