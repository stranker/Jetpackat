script/source = "extends Control

var next_scene = ''
export (PackedScene) var leader
var easter_cont : int = 0

func _ready():
	$VersionLabel.text = \"VERSION 0.8\"
	get_tree().paused = false
	set_items_equipped()
	$Tween.interpolate_property($Background, 'self_modulate', $Background.self_modulate, Color.black, 15, Tween.TRANS_LINEAR,Tween.EASE_IN)
	$Tween.interpolate_property($Stars, 'self_modulate', Color(0,0,0,0), Color.white, 12, Tween.TRANS_LINEAR,Tween.EASE_IN)
	$Tween.start()
	check_first_game()
	GameManager.upload_highscore()
	pass

func check_first_game():
	$NicknamePanel.hide()
	if GameManager.nickname == '':
		$NicknamePanel.show()
		for i in $PawCat.get_children():
			i.disabled = true
	pass

func set_items_equipped():
	$Player/Skin.self_modulate = 'ffffff'
	$Player/Skin/Hat.self_modulate = 'ffffff'
	$Player/Skin/Scarf.self_modulate = 'ffffff'
	$Player/Skin/Jetpack.self_modulate = 'ffffff'
	$Player/Skin/Pattern.self_modulate = 'ffffff'
	if !GameManager.items_equipped.keys().empty():
		for i in GameManager.items_equipped.keys():
			set_player_menu_item(i)
	pass

func set_player_menu_item(item_type):
	var item_info = GameManager.items_equipped[item_type]
	var texture_route = 'Sprites/Cat/'
	if item_type != 'Skin' or item_type != 'Scarf':
		texture_route = 'Sprites/Shop/Cat/'
	if item_type == 'Skin':
		$Player/Skin.self_modulate = item_info[1]
	else:
		if typeof(item_info) != TYPE_ARRAY:
			$Player/Skin.get_node(item_type).texture = load(texture_route + item_type + '/' + item_info + '.png')
		else:
			$Player/Skin.get_node(item_type).texture = load(texture_route + item_type + '/' + item_info[0] + '.png')
			$Player/Skin.get_node(item_type).self_modulate = item_info[1]
	pass

func _on_StartButton_button_down():
	$Anim.play(\"FadeOut\")
	next_scene = 'Scenes/GameScene.tscn'
	GameManager.save_game_data()
	pass # Replace with function body.

func _on_CloseButton_button_down():
	GameManager.save_game_data()
	get_tree().quit()
	pass # Replace with function body.

func _on_CreditsButton_button_down():
	$Anim.play(\"FadeOut\")
	next_scene = 'Scenes/CreditsScene.tscn'
	pass # Replace with function body.

func _on_Anim_animation_finished(anim_name):
	if anim_name == 'FadeOut':
		get_tree().change_scene(next_scene)
	if anim_name == 'Start':
		$Anim.play('Idle')
	if anim_name == 'Easter':
		$Anim.play('Start')
		easter_cont = 0
	pass # Replace with function body.

func _on_ConfigButton_button_down():
	$ConfigPanel.show()
	pass # Replace with function body.

func _on_ShopButton_button_down():
	GameManager.save_game_data()
	$Anim.play(\"FadeOut\")
	next_scene = 'Scenes/ShopScene.tscn'
	pass # Replace with function body.

func _on_LeaderboardButton_button_down():
	get_leaderboard_data()
	$Leaderboard.show()
	pass # Replace with function body.

func get_leaderboard_data():
	if $Leaderboard/Leaders/SC/ItemList.get_children().empty():
		var public_url = 'http://dreamlo.com/lb/5cba6e4a3eba5e041c24a2b4/json'
		$HTTPRequest.request(public_url,[],true,HTTPClient.METHOD_GET)
		$Leaderboard/State.text = 'CONNECTING...'
	pass

func _on_CloseLeaderButton_button_down():
	$Leaderboard.hide()
	pass # Replace with function body.

func _on_HTTPRequest_request_completed(result, response_code, headers, body):
	if result == HTTPRequest.RESULT_SUCCESS:
		$Leaderboard/State.hide()
		var leaderboard_data = JSON.parse(body.get_string_from_utf8()).result
		var leaderboard_list : Dictionary = leaderboard_data['dreamlo']['leaderboard']
		if leaderboard_list:
			fill_leaderboard(leaderboard_list['entry'])
		else:
			fill_leader_empty(1,20)
	else:
		$Leaderboard/State.text = 'FAILED TO CONNECT'
	pass # Replace with function body.

func fill_leaderboard(entries):
	var min_entries = 20
	var empty_leader
	var pos = 1
	if typeof(entries) == TYPE_DICTIONARY:
		empty_leader = min_entries - 1
		var l = leader.instance()
		$Leaderboard/Leaders/SC/ItemList.add_child(l)
		l.set_info(pos,entries)
		pos += 1
		fill_leader_empty(pos,empty_leader)
	if typeof(entries) == TYPE_ARRAY:
		empty_leader = min_entries - entries.size()
		for i in entries:
			var l = leader.instance()
			l.set_info(pos,i)
			$Leaderboard/Leaders/SC/ItemList.add_child(l)
			pos += 1
		fill_leader_empty(pos,empty_leader)

func fill_leader_empty(pos,cant):
	for i in range(pos,cant + 1):
		var l = leader.instance()
		l.set_info(i,{})
		$Leaderboard/Leaders/SC/ItemList.add_child(l)
	pass

func _on_LineEdit_text_changed(new_text):
	$NicknamePanel/AcceptButton.visible = new_text != ''
	pass # Replace with function body.

func _on_AcceptButton_button_down():
	GameManager.nickname = $NicknamePanel/Nickname.text
	$NicknamePanel.hide()
	for i in $PawCat.get_children():
		i.disabled = false
	pass # Replace with function body.

func _on_SC_gui_input(event):
	if event is InputEventScreenDrag:
		if event.is_pressed():
			$Leaderboard/Leaders/SC.scroll_vertical = event.relative.y
	pass # Replace with function body.

func _on_TouchArea_input_event(viewport, event, shape_idx):
	print(event)
	if event is InputEventScreenTouch:
		if event.pressed and event.is_echo() and easter_cont < 5:
			easter_cont += 1
			if easter_cont > 4:
				$Anim.play('Easter')
	pass # Replace with function body.

func _on_PlayerEasterButton_button_down():
	easter_cont += 1
	if easter_cont > 4:
		$Anim.play('Easter')
	pass # Replace with function body."

